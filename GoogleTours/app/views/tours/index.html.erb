<h1>Google Tours</h1>
    <input id = "zipfinder">
    <button id = "zipbutton">Search By Zip</button>
    <button id = "createTour">Create new Tour</button>
    <button id = "ajaxTest">TESTING</button>
    <div id="map"></div>
    <div id = "view-popup" class = "popup">
      <h5>Hit escape to return to the map</h5>
      <div id = 'tour-controls'>
        <button id = 'forward' class = 'tour-control' dataID = '0'>Forwards</button>
        <button id = 'backward' class = 'tour-control' dataID = '1'>Backwards</button>
      </div>
      <div id = 'panoView' class = 'pano'>
      </div>
    </div>

    <div id = "create-popup" class = "popup">
      <h5>Hit escape to return to the map</h5>
      <button id = "createBlurb">Add a blurb</button>
      <input id = "blurbInput"></input>
      <button id = "blurbSave">Save your blurb</button>
      <button id = "addPreview">Add a preview</button>
      <div id = 'panoCreate' class = 'pano'>
        <div id = "panoWriter" class = 'pano'>
        </div>
      </div>
    </div>

    <div id = "preview-popup" class = "popup">
      <button id = "saveTour">Save your tour</button>
      <input id = "previewInput"></div>
    </div>

    <script src="https://maps.googleapis.com/maps/api/js?key=<%= ENV["GMKEY"] %>&callback=initMap"
    async defer></script>
